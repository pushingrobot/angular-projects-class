{
  "name": "angular-projects-course",
  "version": "0.1.0",
  "description": "Angular4 + Express starter project",
  "scripts": {
    "build": "webpack --progress",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --progress --port 4200 --content-base src",
    "express": "concurrently",
    "start": "npm run server",
    "watch-server": "concurrently -k -p \"[{name}]\" -n \"Client,Sass,TypeScript,Node\" -c \"magenta.bold,yellow.bold,cyan.bold,green.bold\" \"npm run watch\" \"npm run watch-sass\" \"npm run watch-ts\" \"nodemon dist/server/server.js\"",
    "start-server": "rimraf dist && npm run build && npm run build-server && npm run build-sass && npm run watch-server",
    "build-server": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node dist/server/server.js",
    "test-server": "jest --forceExit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass server/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w server/public/css/main.scss dist/public/css/main.css",
    "tslint": "tslint --type-check -c tslint.json -p tsconfig.json",
    "copy-static-assets": "node copyStaticAssets.js"
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>",
    "David Stephens <pushingrobot@me.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "4.2.0-beta.1",
    "@angular/common": "^4.2.0-beta.1",
    "@angular/compiler": "^4.2.0-beta.1",
    "@angular/compiler-cli": "^4.2.0-beta.1",
    "@angular/core": "^4.2.0-beta.1",
    "@angular/forms": "^4.2.0-beta.1",
    "@angular/http": "^4.2.0-beta.1",
    "@angular/platform-browser": "^4.2.0-beta.1",
    "@angular/platform-browser-dynamic": "^4.2.0-beta.1",
    "@angular/platform-server": "^4.2.0-beta.1",
    "@angular/router": "^4.2.0-beta.1",
    "@angular/upgrade": "^4.2.0-beta.1",
    "angular-in-memory-web-api": "^0.3.1",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "cors": "^2.0.3",
    "express": "^4.15.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.15.2",
    "express-validator": "^3.2.0",
    "https": "^1.0.0",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@types/compression": "0.0.33",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.18",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.7",
    "to-string-loader": "^1.1.5",
    "tslint": "^5.2.0",
    "tslint-language-service": "^0.9.5",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "keywords": [
    "Angular4",
    "angular4-seed",
    "custom angular 4 seed"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pushingrobot/angular-projects-course.git"
  },
  "bugs": {
    "url": "https://github.com/pushingrobot/angular-projects-course/issues"
  },
  "homepage": "https://github.com/pushingrobot/angular-projects-course#readme"
}
